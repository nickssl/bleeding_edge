function mvn_sep_puo_flux,x  ,angle=angle
  common mvn_sep_puo_flux_com,dat
  if ~ keyword_set(dat) then dat = read_asc(/thisfile,format={e:0.,pa:0.,flux:0.})
  if n_params() eq 0 then return, dat
;  if keyword_set(inverse) then return, interp(dat.temperature,dat.resistance,x,/no_extrap)
  if keyword_set(angle) then return, interp(dat.pa,dat.e,x,/no_extrap)
  return, interp(dat.flux,dat.e,x,/no_extrap)
end


